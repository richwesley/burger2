<div class="container-fluid">
    <div class ="row" id="header">  
		  <h1>Pacman Devours Jelly Bellys</h1>
		</div>
  	<div class ="row">
	  <div class="col-md-3">
  				<img src="assets/img/pacman.png" class="img img-responsive" alt="Pacman">
  	  </div>
  		
	  <div class="col-md-6"> 
	  <video id="video" autoplay loop>
	  	<source src="assets/img/pacman.mp4">
	  </video> 
	  </div>
		
	  <div class="col-md-3" id="jellybean">
			<img src="assets/img/jellybean.jpg" class="img img-responsive" alt="Jelly Belly">
	  </div>

    </div>
  	  
   <div class="row">
   	 <div class="col-md-4">
   	 	<h3>Pacmen</h3>         
        <table class="table table-hover">
          <tbody>
            {{#each eaters}}
           
              <tr>
                <td>{{thid.id}}</td>
                <td>{{this.name}}</td>
              </tr>
            {{/each}}
          </tbody>
        </table>
   	 </div>

      <div class="col-md-4">
   	 	
        <form class="create-update-form" action="/" method="POST">

          <div class="form-group">
            <label for="flavor">Make a New Pacman</label>
            <input type="text" id="pacman" name="pacman">
           
          </div> 

          <button class="btn btn-primary" type="submit">Add New Jelly Belly</button>
      </form>   
   	</div>   